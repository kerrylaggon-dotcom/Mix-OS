{
  "expo": {
    "name": "Mix-OS",
    "slug": "mix-os",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mixos",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mixos.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0D1117",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mixos.app",
      "versionCode": 1,
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0D1117",
          "dark": {
            "backgroundColor": "#0D1117"
          }
        }
      ],
      "expo-web-browser"
    ],
    "assetBundlePatterns": [
      "assets/images/*",
      "assets/fonts/*",
      "assets/kernel/*",
      "assets/rootfs/*",
      "assets/code-server/*",
      "assets/wasm/*",
      "assets/busybox/*"
    ],
    "extra": {
      "eas": {
        "projectId": "8bce99fb-240f-47e0-9134-7d594df56ef1"
      }
    }
  }
}
